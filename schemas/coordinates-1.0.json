{
  "$id": "https://api.clearlydefined.io/schemas/coordinates-1.0.json#",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "coordinates-1.0",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "type",
    "name",
    "revision"
  ],
  "properties": {
    "type": {
      "enum": [
        "npm",
        "crate",
        "git",
        "maven",
        "composer",
        "nuget",
        "gem",
        "go",
        "pod",
        "pypi",
        "sourcearchive",
        "deb",
        "debsrc"
      ],
      "errorMessage": {
        "type": "coordinates.type must be a string"
      }
    },
    "provider": {
      "enum": [
        "npmjs",
        "cocoapods",
        "cratesio",
        "github",
        "gitlab",
        "packagist",
        "golang",
        "mavencentral",
        "mavengoogle",
        "nuget",
        "rubygems",
        "pypi",
        "debian"
      ],
      "errorMessage": {
        "type": "coordinates.provider must be a string"
      }
    },
    "name": {
      "type": "string",
      "errorMessage": {
        "type": "coordinates.name must be a string"
      }
    },
    "namespace": {
      "type": "string",
      "errorMessage": {
        "type": "coordinates.namespace must be a string"
      }
    },
    "revision": {
      "type": "string",
      "errorMessage": {
        "type": "coordinates.revision must be a string"
      }
    }
  }
}